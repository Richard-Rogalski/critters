{
	"manifest_version": 2,
	"name": "Critters",
	"description": "Cute critters on all of your tabs!",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.mozilla.org/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "Critters"
	},
	"options_ui": {
		"page": "options/index.html"
	},
	"chrome_url_overrides": {
		"newtab": "crittertab.html"
	}
}
